<odoo>
    <record id="action_report_journal_entry_excel" model="ir.actions.report">
        <field name="name">Journal Entry Excel</field>
        <field name="model">account.move</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">journal_entry_excel.report_journal_entry_excel</field>
        <field name="report_file">journal_entry_excel.report_journal_entry_excel</field>
        <field name="print_report_name">'JournalEntry-' + object.name + '.xlsx'</field>
    </record>

    <record id="view_move_form_inherit_excel" model="ir.ui.view">
        <field name="name">account.move.form.excel</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <header position="inside">
                <button name="%(action_report_journal_entry_excel)d"
                        string="Print Excel"
                        type="action"
                        class="btn-primary"/>
            </header>
        </field>
    </record>
</odoo>
